{
  "name": "cesium-webpack-example",
  "version": "1.0.0",
  "description": "The minimal recomended setup for an app using Cesium with Webpack.",
  "main": "index.js",
  "keywords": [
    "cesium",
    "webpack",
    "example"
  ],
  "author": {
    "name": "Analytical Graphics, Inc.",
    "url": "https://www.agi.com"
  },
  "homepage": "https://cesiumjs.org",
  "license": "Apache-2.0",
  "devDependencies": {
    "cesium": "^1.98.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "webpack-dev-server --host 0.0.0.0 --config webpack.config.js --open",
